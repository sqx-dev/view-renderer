{
  "name": "@squarex/view-renderer",
  "version": "2.0.8",
  "description": "Client application for interacting with SquareX disposable instances",
  "browser": "lib/rfb",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "test": "tests"
  },
  "files": [
    "lib",
    "AUTHORS",
    "VERSION",
    "docs/API.md",
    "docs/LIBRARY.md",
    "docs/LICENSE*",
    "core",
    "vendor/pako"
  ],
  "scripts": {
    "typecheck": "tsc --watch --project tsconfig.json --preserveWatchOutput",
    "dev": "node ./scripts/dev.js",
    "test": "karma start karma.conf.js",
    "build": "node ./scripts/build.js",
    "build:dev": "node ./scripts/build.js --env=dev",
    "build:public": "node ./scripts/build.js --env=public",
    "create-release": "node ./scripts/create-release.js",
    "make-artifacts": "mkdir -p ./.artifacts/releases && rm -f ./.artifacts/releases/* && rm -f ./.artifacts/*.js && node ./scripts/scripts.esbuild.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SquareX-Labs/extension-view-renderer"
  },
  "author": "SquareX (https://www.sqrx.com)",
  "contributors": [],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/SquareX-Labs/extension-view-renderer/issues"
  },
  "homepage": "https://github.com/SquareX-Labs/extension-view-renderer",
  "devDependencies": {
    "@chiragrupani/karma-chromium-edge-launcher": "*",
    "chai": "*",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "*",
    "core-js": "^3.18.3",
    "eslint": "^8.37.0",
    "fs": "0.0.1-security",
    "fs-extra": "*",
    "jsdom": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-firefox-launcher": "*",
    "karma-ie-launcher": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-safari-launcher": "*",
    "karma-script-launcher": "*",
    "karma-sinon-chai": "*",
    "mocha": "*",
    "node-getopt": "*",
    "po2json": "*",
    "postcss": "7.0.0",
    "prettier": "^2.8.8",
    "sinon": "*",
    "sinon-chai": "^2.8.0"
  },
  "keywords": [
    "vnc",
    "rfb",
    "novnc",
    "websockify"
  ],
  "dependencies": {
    "@types/chrome": "^0.0.239",
    "concurrently": "^8.2.0",
    "dealwith": "^1.2.1",
    "esbuild": "^0.18.17",
    "typescript": "^5.1.5"
  }
}
